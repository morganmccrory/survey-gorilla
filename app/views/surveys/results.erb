<div id="results">
  <h1><%= @survey.title %></h1>
  <h3>results</h3>
  <a href="/surveys">Return to Surveys Page</a>
</div>
<div id="result_questions">
<% @survey.questions.each do |question| %>
<table>
  <thead>
    <tr>
      <td>
        <%= question.prompt %>
      </td>
    </tr>
  </thead>
  <tbody>
    <% question.choices.each do |choice| %>
    <tr>
      <td>
        <%= choice.answer %>
      </td>
      <td>
        <%= choice.responses.count %>
      </td>
    </tr>
    <% end %>
  </tbody>
</table>
<% end %>
</div>

